class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        
        vector<int>count(nums.size()+1 , 0);
        vector<int>nos;
        
        for(int i=0; i<nums.size(); i++){
            count[nums[i]]++;
        }
        
        for(int i=1; i<=nums.size(); i++){
            if(count[i]==0){
                nos.push_back(i);
            }
        }
        
        return nos;
    }
};
